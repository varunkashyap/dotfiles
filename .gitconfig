[log]
	abbrevCommit = yes
[apply]
    whitespace = fix
[core]
	abbrev = 8

[color]
	ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = cyan
    untracked = red

[merge]
    log = true

[url "git://github.com:"]
    insteadOf = "github:"
